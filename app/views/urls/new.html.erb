<h1>New Url</h1>

<%= form_with model: @article do |form| %>
  <div>
    <%= form.label :long_url %><br>
    <%= form.text_field :long_url %>
    <%= @url.errors.full_messages_for(:url_long).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
