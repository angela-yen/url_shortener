<h1>Welcome to Ange's URL Shortener 📏</h1>

<h2 class='form_link'><%= link_to "New Url", new_url_path %></h2>
<h2 class='form_link'><%= link_to "QR Code Generator", qr_code_generator_path %> 


<h3>Past Links</h3>
<ul class='list'>
  <% @urls.each do |url| %>
    <li style="padding: 10px">
     <%= link_to url.short_url, url %>
    </li>
  <% end %>
</ul>

 